{
  "expo": {
    "name": "PlantsGenius",
    "slug": "plant-genius",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.plant-genius",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone",
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos"
      }
    },
    "android": {
      "package": "app.rork.plant-genius",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.VIBRATE",
        "INTERNET"
      ]
    },
    "web": {
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "plantnetApiKey": "",
      "plantnetApiUrl": "https://my-api.plantnet.org/v2/identify/all",
      "mongodbApiUrl": "",
      "googleAndroidClientId": "",
      "googleIosClientId": "",
      "googleWebClientId": "",
      "appleSignInEnabled": false,
      "smtp2goApiUrl": "https://api.smtp2go.com/v3/email/send",
      "paystackPublicKey": "",
      "sentryEnabled": false,
      "appEnv": "development",
      "debugMode": true,
      "router": {},
      "eas": {
        "projectId": "33c35995-efbc-49f6-ae30-63a387d9a06d"
      }
    }
  }
}
